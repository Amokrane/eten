<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/restaurantMapsFragment">
    <fragment
        android:id="@+id/restaurantMapsFragment"
        android:name="com.chentir.maps.RestaurantMapsFragment"
        app:moduleName="maps"
        android:label="restaurant_maps_fragment"
        tools:layout="@layout/restaurant_maps_fragment" >
        <action
            android:id="@+id/action_restaurantMapsFragment_to_restaurantDetailFragment"
            app:destination="@id/restaurantDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/restaurantDetailFragment"
        android:name="com.chentir.detail.RestaurantDetailFragment"
        app:moduleName="detail"
        android:label="restaurant_detail_fragment"
        tools:layout="@layout/restaurant_detail_fragment" />
</navigation>